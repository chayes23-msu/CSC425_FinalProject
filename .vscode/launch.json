    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Server",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\backend\\server.js",
            "envFile": "${workspaceFolder}\\backend\\.env",
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Run dB",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\backend\\database\\db.js",
            "envFile": "${workspaceFolder}\\backend\\.env",
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Run Seeder",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\backend\\database\\seedDb.js",
            "envFile": "${workspaceFolder}\\backend\\.env",
        }
    ]
}